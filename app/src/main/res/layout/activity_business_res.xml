<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".businessPart.BusinessResActivity">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="475dp"
        android:layout_height="833dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.386"
        app:srcCompat="@drawable/bb" />

    <Button
        android:id="@+id/business_tablebutton"
        android:layout_width="125dp"
        android:layout_height="41dp"
        android:layout_marginTop="36dp"
        android:text="Masalar"
        app:layout_constraintBottom_toBottomOf="@+id/business_reservedbutton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.867"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/business_res_peoplenumber"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/business_mapbutton"
        android:layout_width="82dp"
        android:layout_height="42dp"
        android:layout_marginTop="36dp"
        android:text="Map"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/business_res_peoplenumber"
        app:layout_constraintVertical_bias="0.018" />

    <Button
        android:id="@+id/business_dashbutton"
        android:layout_width="127dp"
        android:layout_height="43dp"
        android:layout_marginTop="28dp"
        android:text="Dashboard"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.137"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/business_res_peoplenumber"
        app:layout_constraintVertical_bias="0.129" />

    <TextView
        android:id="@+id/business_res_peoplenumber"
        android:layout_width="242dp"
        android:layout_height="52dp"
        android:layout_marginTop="24dp"
        android:text="Kişi Sayısı"
        android:textColor="#000000"
        android:textColorHint="#0C0C0C"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.402"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/business_res_hour"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/business_res_hour"
        android:layout_width="242dp"
        android:layout_height="52dp"
        android:layout_marginTop="24dp"
        android:text="Rezerve Saati"
        android:textColor="#000000"
        android:textColorHint="#0C0C0C"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/business_reservedbutton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.402"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/business_res_date"
        app:layout_constraintVertical_bias="0.005" />

    <TextView
        android:id="@+id/business_res_date"
        android:layout_width="242dp"
        android:layout_height="52dp"
        android:text="Rezerve Tarihi"
        android:textColor="#000000"
        android:textColorHint="#0C0C0C"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.402"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.48" />

    <TextView
        android:id="@+id/business_res_tableno"
        android:layout_width="242dp"
        android:layout_height="52dp"
        android:layout_marginBottom="12dp"
        android:text="Masa Numarası"
        android:textColor="#000000"
        android:textColorHint="#0C0C0C"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/business_res_date"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.402"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.909" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout"
        android:layout_width="408dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="86dp">

    </com.google.android.material.textfield.TextInputLayout>

    <TextView
        android:id="@+id/business_res_who"
        android:layout_width="242dp"
        android:layout_height="52dp"
        android:text="Rezerve eden kişi "
        android:textColor="#000000"
        android:textColorHint="#0C0C0C"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/business_res_tableno"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.402"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.769" />

    <TextView
        android:id="@+id/business_res_text"
        android:layout_width="148dp"
        android:layout_height="38dp"
        android:text="Rezerve Bilgileri"
        android:textColor="#000000"
        android:textColorHint="#0C0C0C"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.065" />

    <Button
        android:id="@+id/business_reservedbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rezerveler"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.971" />

    <androidx.constraintlayout.widget.Group
        android:layout_width="7dp"
        android:layout_height="31dp"
        tools:layout_editor_absoluteX="151dp"
        tools:layout_editor_absoluteY="475dp" />

</androidx.constraintlayout.widget.ConstraintLayout>